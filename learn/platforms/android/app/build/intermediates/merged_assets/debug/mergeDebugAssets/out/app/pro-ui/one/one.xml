<Page xmlns:au="nativescript-ui-autocomplete" loaded="loaded">
    <ActionBar title="Pro-UI one" />
    <StackLayout class="stack-layout">
        <Label text="{{name}}" fontSize="40" />
        <Label text="Artist:" class="label" />
        <au:RadAutoCompleteTextView items="{{ dataItems }}" suggestMode="Append" displayMode="Token" completionMode="Contains" layoutMode="Horizontal"
        tokenAdded="tokenAdded"
        tokenRemoved = "tokenRemoved"
        textChanged="textChanged"
        id="autoComplete"
         />
